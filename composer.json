{
    "name": "datomatic/laravel-fatture-in-cloud",
    "description": "Laravel wrapper for Fatture in Cloud API v2",
    "keywords": [
        "Datomatic",
        "laravel",
        "fatture-in-cloud",
        "laravel-fatture-in-cloud"
    ],
    "homepage": "https://github.com/datomatic/laravel-fatture-in-cloud",
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto",
            "email": "info@albertoperipolli.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37||^9.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Datomatic\\FattureInCloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datomatic\\FattureInCloud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datomatic\\FattureInCloud\\FattureInCloudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
